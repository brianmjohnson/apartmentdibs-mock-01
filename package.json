{
  "name": "project-name",
  "version": "0.1.0",
  "private": true,
  "description": "Next.js project from template",
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/project-name"
  },
  "scripts": {
    "dev": "pnpx kill-port ${PORT:-3000} && next dev --turbopack",
    "build": "pnpm prebuild && tsc --noEmit && next build",
    "prebuild": "pnpm gen:check",
    "vercel-build": "next info && zenstack generate && pnpm prisma generate && next build && prisma migrate deploy",
    "start": "next start",
    "lint": "prettier --write --cache . && eslint --fix --cache .",
    "lint:check": "prettier --check --cache . && eslint --cache .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "gen:check": "NODE_OPTIONS='--max-old-space-size=8192' zenstack generate && prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:seed": "tsx prisma/seed/index.ts",
    "db:studio": "prisma studio",
    "hitl:resume": "tsx scripts/hitl-resume.ts",
    "session:summary": "tsx scripts/generate-session-summary.ts",
    "diagnose": "bash scripts/diagnose-web-environment.sh",
    "package-clean": "rm -rf tsconfig.tsbuildinfo node_modules .next package-lock.json lib/hooks/generated lib/generated .zenstack && pnpm install && pnpm build"
  },
  "dependencies": {
    "@prisma/client": "^6.16.0",
    "@radix-ui/react-aspect-ratio": "^1.1.8",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@t3-oss/env-nextjs": "^0.11.0",
    "@tanstack/react-query": "^5.62.0",
    "@trpc/client": "next",
    "@trpc/react-query": "next",
    "@trpc/server": "next",
    "better-auth": "^1.3.27",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "consola": "^3.4.2",
    "embla-carousel-react": "^8.6.0",
    "lucide-react": "^0.554.0",
    "next": "^15.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.0",
    "zod-to-json-schema": "^3.24.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@zenstackhq/runtime": "^2.19.2",
    "@zenstackhq/tanstack-query": "^2.19.2",
    "@zenstackhq/trpc": "^2.19.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.0.0",
    "eslint-config-next": "^15.1.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.0",
    "prisma": "^6.16.0",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "zenstack": "^2.19.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.15.0"
}
